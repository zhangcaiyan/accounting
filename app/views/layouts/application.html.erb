<!DOCTYPE html>
<html>
  <head>
    <%= yield(:head) %>
    <%= stylesheet_link_tag :all %>
    <%= javascript_include_tag :defaults %>
    <%= javascript_include_tag "jquery" %>
    <script type="text/javascript">
      jQuery.noConflict();
    </script>
    <%= csrf_meta_tag %>
  </head>
  <body id="main">
    <%= render '/partial/header' %>

    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>

    <%= yield %>

  </body>
</html>
