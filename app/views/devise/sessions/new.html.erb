<%= title "登陆" %>

<%= alert %> 
<%= simple_form_for(resource, as: "user", url: session_path(resource_name), wrapper: :no_label) do |f| %>
  <%= f.input :login %>
  <%= f.input :password, placeholder: '密码' %>
  <% if devise_mapping.rememberable? -%>
    <%= t 'activerecord.attributes.user.remember_me' %>:
    <%= f.check_box :remember_me %>
  <% end %>
  <%= f.button :submit, "登陆" %>
<% end %>

