<%= title "注册" %>

<%= devise_error_messages! %>
<%= simple_form_for(@user, url: registration_path(resource_name), wrapper: :no_label) do |f| %>
  <%= f.input :email %>
  <%= f.input :username %>
  <%= f.input :password %>
  <%= f.input :password_confirmation %>
  <%= f.button :submit %>
<% end %>
  
