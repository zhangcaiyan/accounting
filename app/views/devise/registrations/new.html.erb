<%= title "注册" %>

<%= simple_form_for(@user, url: registration_path(resource_name), wrapper: :no_label) do |f| %>
  <%= f.input :email, placeholder: "电子邮件" %>
  <%= f.input :username, placeholder: "用户名" %>
  <%= f.input :password, placeholder: "密码" %>
  <%= f.input :password_confirmation, placeholder: "密码确认" %>
  <%= f.button :submit %>
<% end %>
  
